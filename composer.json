{
  "name": "mgonzalezbaile/workshop-php-skeleton",
  "description": "Skeleton of a dockerized php application ready to start working",
  "license": "MIT",
  "require": {
    "php": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Skeleton\\": "Skeleton/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload-dev": {
    "psr-4": {
      "SkeletonTest\\": "Skeleton/tests/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "preferred-install": "dist"
  }
}

